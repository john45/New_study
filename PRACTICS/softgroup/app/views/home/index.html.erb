  <% if current_user %>

    <p class="all-users">
      <ul>
        <% @users.each do |user| %>
          <li><%= user.email %></li>
        <% end %>
      </ul>
    </p>

  <% else %>
    <div class="sign_up">
      <p>Sign UP please</p>

      <%= link_to 'Sign UP!', sign_up_path, class: 'btn btn-primary btn-lg' %>
    </div>

  <% end %>

